{% assign tabs_trigger = false %}
{% assign reviews = false %}


{% if section.settings.svg_desktop != blank %}
  <div class="desktop_only curve_top {{ section.settings.custom_class }}">{{ section.settings.svg_desktop }}</div>
  {% endif %}
  {% if section.settings.svg_mobile != blank %}
  <div class="mobile_only curve_top {{ section.settings.custom_class }}">{{ section.settings.svg_mobile }}</div>
  {% endif %}

<div class=" section_bg_{{ section.id }} {{ section.settings.custom_class }}"{% if section.settings.custom_id != blank %} id="{{ section.settings.custom_id }}"{% endif %}
style="background-color:{{ section.settings.bg_color | default:'transparent' }}">
   <div class="container_hk section-{{ section.id }}-padding">
     <div class="richtext_blocks_new {{ section.settings.position }} block_{{ section.id }}" style="background-color:{{ section.settings.bg_color2 | default:'transparent' }};color:{{ section.settings.color | default:'#000' }}">
        {%- for block in section.blocks -%}
          {%- case block.type -%}
          
            {% when 'tab_trigger' %}
            {% if tabs_trigger == false %}
{% assign tabs_trigger = true %}
       
          {% assign first_tab_id = "" %}
          {% assign tab_loop = 0 %}
               <div class="trigger_tabs_main tabs_{{ section.id }}">
        <div class="trigger_tabs_flex">
          {% assign first_tab_id = "" %}
          {% assign tab_loop = 0 %}
        {%- for block in section.blocks -%}
          {%- case block.type -%}
            {% when 'tab_trigger' %}
  {% assign tab_loop = tab_loop | plus: 1 %}
            {% if tab_loop == 1 %}
          {% assign first_tab_id = block.settings.id %}
              {% endif %}
        <div class="trigger_tabs_item_inner">
        <div class="trigger_tabs_item{% if tab_loop == 1 %} active{% endif %}" data_tab="{{ block.settings.id }}">
          {% if block.settings.image != blank %}
             <img src="{{ block.settings.image | image_url }}" alt="{{ block.settings.image.alt }}" width="auto" height="auto" fetchpriority="high" loading="eager">
      {% endif %}
            <div class="tab_texts">
          {% if block.settings.title != blank %}
            <div class="tab_title">{{ block.settings.title }}</div>
              {% endif %}
          {% if block.settings.text != blank %}
            <div class="tab_text">{{ block.settings.text }}</div>
              {% endif %}
            </div>
        </div>
        </div>
            {% endcase %}
          {% endfor %}
        </div>
        </div>
<style>
  .{{section.settings.trigger_class}}:not(#{{first_tab_id}}) {
    display:none;
  }
</style>
<script>
 jQuery('.tabs_{{ section.id }} .trigger_tabs_item').click(function(){
    jQuery('.tabs_{{ section.id }} .trigger_tabs_item').removeClass('active');
   jQuery(this).addClass('active');
    var id = jQuery(this).attr('data_tab');
    jQuery('.{{ section.settings.trigger_class }}').hide();
    jQuery('#'+id).show();
    jQuery('#'+id).find('.slick-slider').slick('refresh');
   window.scrollTo(window.scrollX, window.scrollY - 1);
window.scrollTo(window.scrollX, window.scrollY + 1);
});
</script>
