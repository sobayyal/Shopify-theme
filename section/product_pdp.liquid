            {%- when 'heading' -%}
              <h2 class="rich-text__heading  inline-richtext {{ block.settings.custom_class }} {{ block.settings.heading_size }}" {{ block.shopify_attributes }}>{{ block.settings.text }}</h2> 
            {%- when 'richtext' -%}
              <div  class="rich-text__text  {{ block.settings.custom_class }}" {{ block.shopify_attributes }}>{{ block.settings.text | replace:'<li>','<li><svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" viewBox="0 0 30 30" fill="none">
  <path fill-rule="evenodd" clip-rule="evenodd" d="M14.6998 26.1242C20.7887 26.1242 25.7248 21.1882 25.7248 15.0992C25.7248 9.01028 20.7887 4.07422 14.6998 4.07422C8.61087 4.07422 3.6748 9.01028 3.6748 15.0992C3.6748 21.1882 8.61087 26.1242 14.6998 26.1242ZM14.5503 19.6707L20.6753 12.3207L18.5242 10.5282L13.3807 16.7004L10.7897 14.1095L8.80981 16.0894L12.4848 19.7644L13.5688 20.8484L14.5503 19.6707Z" fill="#0066D3"/>
</svg>' }}</div>
            
            {%- when 'text' -%}
              <div  class="rich-text__text  {{ block.settings.custom_class }}" {{ block.shopify_attributes }}>{{ block.settings.text }}</div>
            
            {%- when 'html' -%}
              <div class="rich-text__text  {{ block.settings.custom_class }}" {{ block.shopify_attributes }}>{{ block.settings.text }}</div>
            {%- when 'stock' -%}
            
            <div class="rich-text__text {{ block.settings.custom_class }}">
{% assign date = "now" | date: '%s' %}
{% assign first_date = 3 | times: 24 | times: 60 | times: 60 %}
                   <div class="in_stock_text">
                    <span class="stock_outer"><span class="stock_inner"></span></span>
                    <p>{{ block.settings.text }}<span>{{ date | date: "%s" | plus: first_date | date: "%B %dth" }}</span></p>
                    </div> 
              </div>
            
            
            
        {%- when 'rating_custom' -%}
 <div class="rich-text__text product_rating_lp {{ block.settings.custom_class }}">
<div class="reviews_rating_html">
<a href="#reviews">
{% if block.settings.image != blank %}
<img src="{{ block.settings.image | image_url }}" alt="{{ block.settings.image.alt }}" width="auto" height="auto" loading="lazy">
{% endif %}
            {{ block.settings.star }}
<span>{{ block.settings.text }}</span>
</a>
</div>
 </div>
            {%- when 'faq' -%}
       {% if block.settings.question != blank %}
