$(document).on("click", '.quantity_item', function(e) {  
$('.quantity_item').removeClass('active');
$(this).addClass('active');  
var quantity = $(this).attr('quantity');
var data_tab = $(this).attr('data_tab');
$('.trigger_{{ section.id }}').attr('quantity',quantity);
$('.price_trigger').hide().removeClass('active');
$('.price_trigger[data_tab="'+data_tab+'"]').show();
var selected_text = $(this).attr('label');
$('.product_text_3 strong').text(selected_text);
});
    
$(document).on("click", '.subscription_item', function(e) {
$('.subscription_item').removeClass('active');  
$(this).addClass('active');  
var selected_tab = $(this).attr('data_tab');
if (selected_tab == '2'){
var selling_plan = $(this).attr('selling_plan');
$('.trigger_{{ section.id }}').attr('selling_plan',selling_plan);
} else {
$('.trigger_{{ section.id }}').removeAttr('selling_plan');
}
$('.subscription_trigger').hide();
$('.subscription_trigger[data_tab="'+selected_tab+'"]').show();
});
  
