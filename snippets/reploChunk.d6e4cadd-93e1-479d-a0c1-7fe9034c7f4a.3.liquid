$(document).ready(function () {
  // Wait for Replo to render dynamic nodes
  const wait = setInterval(function () {
    const $bar = $('[data-class="stick_atc"]');
    const $product = $('#product');
    if (!$bar.length || !$product.length) return;

    clearInterval(wait);
    $bar.hide(); // start hidden

    let lastST = $(window).scrollTop();

    function ratios($el) {
      const winTop = $(window).scrollTop();
      const winBottom = winTop + $(window).height();
      const elTop = $el.offset().top;
      const elH = $el.outerHeight();
      const elBottom = elTop + elH;

      // How much of the element has passed above the viewport top (0..1)
      const scrolledPastTop = Math.max(0, Math.min(1, (winTop - elTop) / elH));

      // How much of the element is visible in viewport (0..1)
      const overlap = Math.max(0, Math.min(winBottom, elBottom) - Math.max(winTop, elTop));
      const visibleRatio = overlap / elH;

      return { winTop, winBottom, elTop, elBottom, elH, scrolledPastTop, visibleRatio };
    }

    function onScroll() {
      const st = $(window).scrollTop();
      const dirDown = st > lastST;
      lastST = st;

      const r = ratios($product);

      // SHOW: when scrolling down and at least 75% of the element is above the viewport top
      if (dirDown && r.scrolledPastTop >= 0.75) {
        if (!$bar.is(':visible')) $bar.fadeIn(130);
        return;
      }

      // HIDE: when scrolling up and at least 25% is visible from the bottom
      // (i.e., we're seeing the bottom portion; top is still above the viewport)
      const bottomOnlyVisible = (r.elTop < r.winTop); // top is above viewport
      if (!dirDown && r.visibleRatio >= 0.25 && bottomOnlyVisible) {
        if ($bar.is(':visible')) $bar.fadeOut(130);
        return;
      }
    }

    $(window).on('scroll resize', onScroll);
    onScroll();

    // Optional: make it fixed at the bottom if not already styled
    $bar.css({ position: 'fixed', left: 0, right: 0, bottom: 0, zIndex: 9999 });
  }, 250);
});
</script>
