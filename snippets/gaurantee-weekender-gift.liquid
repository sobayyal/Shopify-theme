<div class="free-gift-block">
      
      
      <div class="save56-text-size-xtiny">
        {% if block.settings.icon_image %}
          <div class="inner__image">
              <img src="{{ block.settings.icon_image |  image_url: width: 20 }}" />
          </div>
        {% endif %}
        <span class="save--text">
        {{ block.settings.bold_text }}
        {{ sale_name }}
        </span>
      </div>

    <div data-gift="container" class="save56-gift-blocks" bis_skin_checked="1">
        <div class="save56-gift-card is-active" data-gift-id="{{ block.settings.gift_one_id }}" data-gift="1" bis_skin_checked="1">
          <img src="{{ gift_img_one | img_url: '300x300' }}" class="save56-gift-image" loading="lazy" style="padding: 12px 11px;">
          <div class="save56-gift-overlay is-active" data-gift="1" bis_skin_checked="1"></div>
          <div class="save56-gift-locker" bis_skin_checked="1">
            <div class="save56-gift-lockings" bis_skin_checked="1">
              {% if block.settings.lock_image %}
              <img src="{{ block.settings.lock_image | img_url : 'master' }}" loading="lazy">
              {% endif %}
            </div>
            <div class="save56-gift-unlockers" bis_skin_checked="1">{{ block.settings.unlock_text_one }}</div>
          </div>
          <div class="save56-gift-price-badge" bis_skin_checked="1">
            <div class="save56-text-block" bis_skin_checked="1">Free <span class="save56-strikethroughs">{{ gift_box_one_price }}</span></div>
          </div>
        </div>
        <!--Face Wipes-->
        
        
        <div class="save56-gift-card is-active" data-gift-id="{{ block.settings.gift_two_id }}" data-gift="2" bis_skin_checked="1">
          <img src="{{ gift_img_two | img_url: '300x300' }}" class="save56-gift-image" loading="lazy" style="padding: 12px 11px;">
          <div class="save56-gift-overlay is-active" data-gift="2" bis_skin_checked="1"></div>
          <div class="save56-gift-locker" bis_skin_checked="1">
            <div class="save56-gift-lockings" bis_skin_checked="1">
              {% if block.settings.lock_image %}
              <img src="{{ block.settings.lock_image | img_url : 'master' }}" loading="lazy">
              {% endif %}
            </div>
            <div class="save56-gift-unlockers" bis_skin_checked="1">{{ block.settings.unlock_text_two }}</div>
          </div>
          <div class="save56-gift-price-badge" bis_skin_checked="1">
            <div class="save56-text-block" bis_skin_checked="1">Free <span class="save56-strikethroughs">{{ gift_box_two_price }}</span></div>
          </div>
        </div>
        
    
        <div class="save56-gift-card is-active" data-gift-id="{{ block.settings.gift_three_id }}" data-gift="3" bis_skin_checked="1">
          <img src="{{ gift_img_three | img_url: '300x300' }}" class="save56-gift-image" loading="lazy">
          <div class="save56-gift-overlay is-active" data-gift="3" bis_skin_checked="1"></div>
          <div class="save56-gift-locker" bis_skin_checked="1">
            <div class="save56-gift-lockings" bis_skin_checked="1">
              {% if block.settings.lock_image %}
              <img src="{{ block.settings.lock_image | img_url : 'master' }}" loading="lazy">
              {% endif %}
            </div>
            <div class="save56-gift-unlockers" bis_skin_checked="1">{{ block.settings.unlock_text_three }}</div>
          </div>
          <div class="save56-gift-price-badge" bis_skin_checked="1">
            <div class="save56-text-block" bis_skin_checked="1">Free <span class="save56-strikethroughs">{{ gift_box_three_price }}</span></div>
          </div>
        </div>
    
      </div>

      <div class="save56-gift-titles" bis_skin_checked="1">
        <div class="save56-gift-title is-active" data-gift-id="{{ block.settings.gift_one_id }}" data-gift="1" bis_skin_checked="1">
          <div class="save56-gift-title-text" bis_skin_checked="1" id="{{ gift_heading_one }}">{{ gift_heading_one }}</div>
        </div>
        
   
        <div class="save56-gift-title is-active" data-gift-id="{{ block.settings.gift_two_id }}" data-gift="2" bis_skin_checked="1">
          <div class="save56-gift-title-text" bis_skin_checked="1" id="{{ gift_heading_two }}">{{ gift_heading_two }}</div>
        </div>
        
        <div class="save56-gift-title is-active" data-gift-id="{{ block.settings.gift_three_id }}" data-gift="3" bis_skin_checked="1">
          <div class="save56-gift-title-text" bis_skin_checked="1" id="{{ gift_heading_three }}">{{ gift_heading_three }}</div>
        </div>
      </div>
      
      <div class="save56-product-breakline" bis_skin_checked="1" style="display:none;">
        <hr>
      </div>
  </div>  

