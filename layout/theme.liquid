<!-- Audiance Tracking from Best Luggage website -->
<script>
(function() {
  const utmParams = ['utm_source', 'utm_medium', 'utm_campaign', 'utm_content', 'utm_term'];
  const domain = window.location.hostname;

  // Get current UTMs from URL
  const urlParams = new URLSearchParams(window.location.search);
  let utmString = '';

  // If URL has UTMs now, store them
  if (utmParams.some(param => urlParams.has(param))) {
    utmString = '?' + utmParams
      .filter(param => urlParams.has(param))
      .map(param => `${param}=${encodeURIComponent(urlParams.get(param))}`)
      .join('&');
    sessionStorage.setItem('stickyUTM', utmString);
  } else if (sessionStorage.getItem('stickyUTM')) {
    utmString = sessionStorage.getItem('stickyUTM');
  }

  // If we have stored UTMs, update all internal links
  if (utmString) {
    document.querySelectorAll('a[href]').forEach(link => {
      let href = link.getAttribute('href');

      // Internal relative links
      if (href && href.startsWith('/') && !href.includes('utm_') && !href.includes('/checkout') && !href.includes('/account')) {
        link.setAttribute('href', href + (href.includes('?') ? '&' : '?') + utmString.substring(1));
      }

      // Absolute internal links
      if (href && href.startsWith(`https://${domain}`) && !href.includes('utm_') && !href.includes('/checkout') && !href.includes('/account')) {
        const urlObj = new URL(href);
        urlObj.search += (urlObj.search ? '&' : '') + utmString.substring(1);
        link.setAttribute('href', urlObj.toString());
      }
    });

    // Replace current URL in browser bar with UTMs if missing
    if (!window.location.search.includes('utm_')) {
      window.history.replaceState({}, '', window.location.pathname + utmString + window.location.hash);
    }
  }
})();
</script>

