    <script>
    document.addEventListener("DOMContentLoaded", function () {
    const cartDrawer = document.querySelector("cart-drawer");

    if (cartDrawer) {
        // Wait for Shadow DOM to be available
        const checkShadowDom = setInterval(() => {
            if (cartDrawer.shadowRoot) {
                clearInterval(checkShadowDom);

                // Select the close button inside the Shadow DOM
                const closeButton = cartDrawer.shadowRoot.querySelector("[is='dialog-close-button']");

                if (closeButton) {
                    closeButton.addEventListener("click", function () {
                        // Remove "open" attribute
                        cartDrawer.removeAttribute("open");

                        // Update aria-modal to false
                        cartDrawer.setAttribute("aria-modal", "false");

                        // Restore scrolling
                        document.body.style.overflow = "";
                        document.documentElement.style.overflow = "";
                    });
                }
            }
        }, 100);
    }
});


    </script>
