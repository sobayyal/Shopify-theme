    {% when 'july-9-28-inch-aio', 'dec-15-large-all-in-one', 'duo-carry', 'cubes-bottle-bundle', 'carry-smart-set', 'ultimate-travel-set', 'check-in-duo', 'c-weekender-bottle-bundle', 'feb-23-aluminum-large', 'collapsible-carry-on', 'final-collapsible-large', 'check-in-24', 'a-b-test', 'bundle-product', 'expandable', 'nobl-duo-bundle' %}
      {{ 'multiple-product-pages.css' | asset_url | stylesheet_tag }}
